<script type="text/javascript">
     class CreateUser{
         constructor(name) {
             this.name=name;
             //this.getName();     
         }
         getName(){
             return this.name;
         }
         setName(str){
             return this.name=str;
         }
         tipAlert(info){
             alert(info);
         }
     }
     var proxyMode=(function(){
         var instance=null;
         return function(name){
             if(!instance){
                 instance=new CreateUser(name);
             }
             return instance;
         }
     })();
     //测试单例类
     var a=new proxyMode('aaaaa');
     var b=new proxyMode('cccc');
     b.setName('2222');
     b.tipAlert('gamecc666');
     console.log(a.getName());
     console.log(b.getName());
</script>
